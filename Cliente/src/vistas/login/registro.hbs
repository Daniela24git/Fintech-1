<link rel="stylesheet" href="/css/login/registro.css">
<div id="contenedor">
  <h1 id="h1">Registrate</h1>
  <form action="/Registro" method="POST">
    <div id="Nombre">
      <label>Nombre :</label>
      <input type="text" name="Nombres" id="nombre"><br>
    </div>
    <div id="Cedula">
      <label>Cedula :</label>
      <input type="number" name="username" id="cedula"><br>
    </div>
    <div id="Contraseña">
      <label>Contraseña:</label>
      <input type="password" name="password" id="contrasena"><br>
    </div>
    <button type="submit" id="button">Ingresar</button> <button id="button2"><a href="/">Salir</a></button>
  </form>
</div>