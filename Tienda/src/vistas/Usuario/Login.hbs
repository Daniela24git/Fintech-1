<LINK REL=StyleSheet HREF="/css/login/ingreso.css" TYPE="text/css" MEDIA=screen>
<div id="imegen">
  Login
</div>
<form action="/Login" method="POST" id="ingresar">
    <div id="contenedor">
      <div id="datos">
      <label id="Usuario">Usuario:</label>
      <input type="text" id="Nombre" name="username">
      </div>
      <div>
        <label id="contraseña">Contraseña: </label>
        <input type="password" id="Contraseña" name="password">
      </div>
      <div>
        <button type="submit" id="boton">Ingresar</button>
      </div>
    </div>
</form>
<button id="sesion"><a href="/Registro">Registro</a></button>